@model IEnumerable<IoTPoC.Data.Model.LogParams>

@{
    ViewBag.Title = "ViewLogParams";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ViewLogParams</h2>

<p>
    @Html.ActionLink("Return to Activity Log list", "Index")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Param)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Value)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Param)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Value)
        </td>
    </tr>
}

</table>
